CONFIG CPD = OFF ; Data Code Protection bit (Data memory code protection is disabled)

CONFIG BOREN = OFF ; Brown Out Reset Selection bits (BOR disabled)
CONFIG IESO = OFF ; Internal External Switchover bit (Internal/External Switchover mode is disabled)
CONFIG FCMEN = OFF ; Fail-Safe Clock Monitor Enabled bit (Fail-Safe Clock Monitor is disabled)
CONFIG LVP = OFF ; Low Voltage Programming Enable bit (((PORTB) and 07Fh), 3/PGM pin has PGM function, low voltage programming enabled)

; CONFIG2
CONFIG BOR4V = BOR40V ; Brown-out Reset Selection bit (Brown-out Reset set to 4.0V)
CONFIG WRT = OFF ; Flash Program Memory Self Write Enable bits (Write protection off)

;-------------------------------------------------------------------------------
; VALORES ABSOLUTOS ASIGNADOS
;-------------------------------------------------------------------------------
LED_HORA EQU 0 ; Utilizado para encender ((PORTA) and 07Fh), 0
LED_FECHA EQU 1 ; Utilizado para encender ((PORTA) and 07Fh), 1
LED_TEMP EQU 2 ; Utilizado para encender ((PORTA) and 07Fh), 2
LED_ACTIVE EQU 3 ; Utilizado para encender ((PORTA) and 07Fh), 3
LED_PUNTOS EQU 4 ; Utilizado para encender ((PORTA) and 07Fh), 4
LED_DISP1 EQU 6 ; Utilizado para encender ((PORTA) and 07Fh), 6
LED_DISP2 EQU 7 ; Utilizado para encender ((PORTA) and 07Fh), 7

LED_FIN_T EQU 0 ; Utilizado para encender ((PORTE) and 07Fh), 0

BT_UP EQU 0 ; Utilizado para leer ((PORTB) and 07Fh), 0
BT_DOWN EQU 1 ; Utilizado para leer ((PORTB) and 07Fh), 1
BT_EDITAR EQU 2 ; Utilizado para leer ((PORTB) and 07Fh), 2
BT_INICIAR EQU 3 ; Utilizado para leer ((PORTB) and 07Fh), 3
BT_MODO EQU 4 ; Utilizado para leer ((PORTB) and 07Fh), 4

SEL_UNIDAD EQU 3 ; Utilizado para encender ((PORTD) and 07Fh), 3
SEL_DECENA EQU 2 ; Utilizado para encender ((PORTD) and 07Fh), 2
SEL_CENTENA EQU 1 ; Utilizado para encender ((PORTD) and 07Fh), 1
SEL_MILES EQU 0 ; Utilizado para encender ((PORTD) and 07Fh), 0

;-------------------------------------------------------------------------------
; MACROS
;-------------------------------------------------------------------------------
RESET_TMR0 MACRO
    BANKSEL TMR0 ; Cambiamos al banco 1
    MOVLW 236 ; Se mueve N al registro W, N=256-((5 ms)(4 MHz)/4*256) -> N= 236 aprox
    MOVWF TMR0 ; Se le da delay a TMR0
    BCF ((INTCON) and 07Fh), 2 ; Limpiamos la bandera de interrupcin

    ENDM

RESET_TMR1 MACRO TMR1_H, TMR1_L
    BANKSEL TMR1H
    MOVLW TMR1_H ; Literal a guardar en TMR1H a W
    MOVWF TMR1H ; Guardamos literal en TMR1H
    MOVLW TMR1_L ; Literal a guardar en TMR1L a W
    MOVWF TMR1L ; Guardamos literal en TMR1L
    BCF ((PIR1) and 07Fh), 0 ; Limpiamos bandera de interrupcin de TMR1

    ENDM

SEL_DISPLAY MACRO DISP1, DISP2, DISP3, DISP4
    MOVF DISP1, W
    CALL TABLA
    MOVWF DISPLAY ; Configuracin para 7 segmentos de unidades

    MOVF DISP2, W
    CALL TABLA
    MOVWF DISPLAY+1 ; Configuracin para 7 segmentos de decenas

    MOVF DISP3, W
    CALL TABLA
    MOVWF DISPLAY+2 ; Configuracin para 7 segmentos de centenas

    MOVF DISP4, W
    CALL TABLA
    MOVWF DISPLAY+3 ; Configuracin para 7 segmentos de miles

    ENDM

;-------------------------------------------------------------------------------
; VARIABLES UTILIZADAS
;-------------------------------------------------------------------------------
PSECT udata_shr ; Common memory
   W_TEMP: DS 1 ; 1 Byte
   STATUS_TEMP: DS 1 ; 1 Byte

PSECT udata_bank0
    MODO: DS 1 ; 1 Byte
    ST_SET: DS 1 ; 1 Byte
    ST_INICIAR: DS 1 ; 1 Byte

    CONT_TMR_1: DS 1 ; 1 Byte
    UNIDADES: DS 1 ; 1 Byte
    DECENAS: DS 1 ; 1 Byte
    CENTENAS: DS 1 ; 1 Byte
    MILES: DS 1 ; 1 Byte

    CONT_TIMER: DS 1 ; 1 Byte
    CONT_FIN_ALARMA: DS 1 ; 1 Byte
    BANDERA_TIMER: DS 1 ; 1 Byte
