    DECENAS_TEMP: DS 1 ; 1 Byte
    CENTENAS_TEMP: DS 1 ; 1 Byte
    MILES_TEMP: DS 1 ; 1 Byte

    MEDIO_SEC: DS 1 ; 1 Byte
    SEGUNDOS: DS 1 ; 1 Byte
    DIV: DS 1 ; 1 Byte
    DIV2: DS 1 ; 1 Byte

